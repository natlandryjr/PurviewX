    "deploy:gcc": "./deploy-production.sh gcc",
    "deploy:gcc-high": "./deploy-production.sh gcc-high"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@google/genai": "^1.26.0",
    "@playwright/test": "^1.56.1",
    "serve": "^14.2.1",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}